{
  "name": "@enterprise/security-middleware",
  "version": "1.0.0",
  "description": "Enterprise-ready security middleware providing consistent secure environment across organizations",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
